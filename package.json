{
  "name": "mutation-summary",
  "version": "0.0.0",
  "description": "Makes observing the DOM fast and easy",
  "main": "src/mutation-summary.js",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "watch": {
    "test": {
      "patterns": ["src", "test"],
      "extensions": "ts"
    }
  },
  "scripts": {
    "build": "webpack --mode production",
    "test": "webpack --mode development && mocha-chrome dist/test/index.html",
    "watch": "npm-watch test"
  },
  "repository": {
    "type": "git",
    "url": "https://code.google.com/p/mutation-summary/"
  },
  "author": "",
  "license": "Apache 2.0",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "chai": "*",
    "file-loader": "^3.0.1",
    "mocha": "^6.0.2",
    "mocha-chrome": "^2.0.0",
    "npm-watch": "^0.6.0",
    "requirejs": "^2.3.6",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "@types/jsdom": "^12.2.3",
    "tslib": "^1.9.3"
  }
}
